{
  "name": "seu-traje",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "nodemon bin/www",
    "startDevelopment": "NODE_PATH=. NODE_CONFIG_DIR=./config nodemon bin/www",
    "startStaging": "NODE_PATH=. NODE_CONFIG_DIR=./config pm2 start bin/www -i 2",
    "preinstallDevelopment": "rm -rf node_modules",
    "installDevelopment": "npm install",
    "postinstallDevelopment": "npm install -g nodemon",
    "preinstallStaging": "rm -rf node_modules",
    "installStaging": "npm install",
    "postinstallStaging": "npm install -g pm2"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "~1.4.3",
    "cookie-session": "^2.0.0-alpha.2",
    "debug": "^2.2.0",
    "ejs": "~2.5.2",
    "express": "~4.14.0",
    "express-session": "^1.15.0",
    "mongoose": "^4.8.4",
    "morgan": "~1.7.0",
    "node-session": "^1.2.1",
    "open": "0.0.5",
    "sendgrid": "^4.7.1",
    "serve-favicon": "~2.3.0"
  }
}
